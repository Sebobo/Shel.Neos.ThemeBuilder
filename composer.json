{
  "name": "shel/neos-themebuilder",
  "type": "neos-plugin",
  "description": "A plugin for NeosCMS which provides integrator/editor tools to define and use themes.",
  "license": "GPL-3.0",
  "keywords": [
    "neos",
    "neoscms",
    "themebuilder",
    "theme",
    "colorpicker",
    "editor"
  ],
  "authors": [
    {
      "name": "Sebastian Helzle",
      "email": "sebastian@helzle.it",
      "homepage": "https://www.helzle.it",
      "role": "Developer"
    }
  ],
  "require": {
    "neos/neos": "^8.3"
  },
  "autoload": {
    "psr-4": {
      "Shel\\Neos\\ThemeBuilder\\": "Classes"
    }
  },
  "extra": {
    "neos": {
      "package-key": "Shel.Neos.ThemeBuilder"
    }
  }
}
